                        left:offsetL,
                         bottom:'-40px',
                        'margin-bottom':'-17px',
                        'z-index':'1002',
                        'width':'1000px'
                    });
                $(this).animate({
                    bottom: '+=40px'
                }, 500);
                $(this).find('a').css('margin-left','10px');


                var isShown = true;
                    $(window).scroll(function () {

                        if ($(window).scrollTop() + wHeight <= height-20) {
                            if(!isShown){
                                isShown = true;
                                $(scrollDiv).show();
                            $(scrollDiv).css({
                                position:'fixed',
                                'padding-top': '7px',
                                'background': 'url(/images/plashka.png)',
                                bottom:'-40px',
                                left:offsetL,
                                'margin-bottom':'-17px',
                                'z-index':'1002'
                            });
                            $(scrollDiv).find('a').css('margin-left','10px');
                            $(scrollDiv).animate({
                                bottom: '+=40px'
                            }, 500);

                            }
                        }
                        else if($(window).scrollTop() + wHeight > height-10){
                            if(isShown){
                                isShown = false;
                            $(scrollDiv).animate({
                                bottom: '-=40px'
                            }, 400,function(){
                                $(scrollDiv).hide();
                                $(scrollDiv).css({
                                    position:'relative',
                                    left:offsetL,
                                    bottom:0,
                                    'background': 'none',
                                    'margin-bottom':'0',
                                    'z-index':1
                                });
                                $(scrollDiv).find('a').css('margin-left','6px');

                            });
                            }
                        }

                    });
                    $(window).resize(function(){
                        offsetL = $('div.container').offset().left;
                        $(scrollDiv).css({
                            left:offsetL
                        });
                    });
            }
</script>
<script>

    function getnewCapcha(ob) {
        var formid = "#" + $(ob).parents("form").attr("ID");

        $.ajax({
            type: "POST",
            url: "/bitrix/getnewcapcha.php",
            dataType: "json",
            data: "GETCAPCHA=Y",
            success: function (data, textStatus) {
                $.each(data, function (key, val) {

                    switch (key) {
                        case "captcha_sid":
                            $(formid + " .captcha_sid").val(val);
                            break;

                        case "capchaimg":
                            $(formid + " .capchaimg").attr("src", val);
                            break;
                    }

                });

            }

        });


        return false;

    }
    function submit_form() {
        var name = $('#u_name').val();

        var mail = $('#u_mail').val();

        var phone = $('#u_phone').val();

        var topic = $('#u_topic').val();

        var message = $('#u_message').val();

        var captcha = $('#u_captcha').val();

        var sid = $('#captcha_sid').val();
        var type = $('#u_type').val();

        $.ajax({
            type: "POST",
            url: "index.php",
            dataType: "json",
            data: {
                U_NAME: name,
                U_MAIL: mail,
                U_PHONE: phone,
                U_TOPIC: topic,
                U_MESSAGE: message,
                captcha_word: captcha,
                captcha_code: sid,
                U_TYPE: type,
                subm_feedback: 'yes'
            },
            success: function (data) {
                getnewCapcha('.addr-ref');
                success_submit = false;
                $.each(data, function (key, val) {

                    if (key == 'SUCCESS') {

                        $('#u_name , #u_phone , #u_mail , #u_captcha , #u_message').removeClass('wrong');
                        success_submit = true;

                        $('#modalWindow .addr-quest-row .addr-comment').fadeOut(1).delay(5400).fadeIn(1);
                        $('#modalWindow ._success').fadeIn().delay(5000).fadeOut(1);
                        return;

                    }

                    if (val == 'N') {
                        $('#' + key).addClass('wrong');
                    } else {
                        if (val == 'Y') $('#' + key).removeClass('wrong');
                    }
                });
                if (!success_submit) {
                    $('#modalWindow .addr-quest-row .addr-comment').fadeOut(1).delay(5400).fadeIn(1);
                    $('#modalWindow .error').fadeIn().delay(5000).fadeOut(1);
                }
            }

        });

        return false;

    }
    /* $('document').ready(function(){
     $('#u_type').sSelect();
     });*/

</script>
